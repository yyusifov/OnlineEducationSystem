<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.onlineeducationsystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OnlineEducationSystem"
        tools:targetApi="31">
        <activity
            android:name=".CourseDescription"
            android:exported="false" />
        <activity
            android:name=".FeedbackSection"
            android:exported="false" />
        <activity
            android:name=".GradeSection"
            android:exported="false" />
        <activity
            android:name=".TestSection"
            android:exported="false" />
        <activity
            android:name=".SubtopicSection"
            android:exported="false" />
        <activity
            android:name=".Lectures"
            android:exported="false" />
        <activity
            android:name=".QuizResult"
            android:exported="false" />
        <activity
            android:name=".PracticeTest"
            android:exported="false" />
        <activity
            android:name=".HomePage"
            android:exported="false" />
        <activity
            android:name=".ResetPassword"
            android:exported="false" />
        <activity
            android:name=".PasswordRecovery"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>